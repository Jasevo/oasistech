# Database (PostgreSQL on Dokploy)
DATABASE_URI=postgresql://user:password@host:5432/task_engine

# Payload Secret (min 32 characters, generate a random string)
PAYLOAD_SECRET=change-me-to-a-random-secret-at-least-32-chars

# Dashboard display name for greeting
DASHBOARD_USER_NAME=Admin

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# API Key for external REST API consumers (generated in Payload admin panel)
TASK_ENGINE_API_KEY=
